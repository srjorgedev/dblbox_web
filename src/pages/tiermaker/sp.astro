---
import Tier from "../../components/tiermaker/Tier.astro";
import Layout from "../../layouts/Layout.astro";
import { getSparkings } from "../../services/getData";

type PathName = "he" | "ex" | "sp" | "ul" | "ll" | "sp&ul" | "ll&ul";

const characters = await getSparkings();

const path = Astro.url.pathname.split("/");
const rarPath = path[2] as PathName;
---

<Layout Selected={2} ICO="default" Title="DBL BOX - Tiermaker">
    <Tier characters={characters} rarPath={rarPath.toString()} />
</Layout>
